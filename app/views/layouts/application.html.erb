<!DOCTYPE html>
<html>
<head>
  <title>KwikEMart</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  
  <%= csrf_meta_tags %>
</head>
<body>
	<% flash.each do |type, message| %>
		<p class="<%= type %>">
			<%= message %>
		</p>
	<% end %>

<%= yield %>
<%= javascript_include_tag "application" %>

</body>
</html>
